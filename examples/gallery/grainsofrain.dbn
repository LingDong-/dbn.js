// Grains of rain -- James Seo// code from DBNnumber mod x y{   value (x-(y*(x/y)))}set rseed <time 4>number random{   set r1 (rseed*435437)   set r2 (rseed+945878)   set rseed <mod r1 r2>   value <mod rseed 100>}number init{   // fudge a little   value (<random>-10)}repeat i 0 100{   set [i 100] <init>   // fudge a little   smaller? [i 100] 20   {      set [i 100] <init>   }}set x 99repeat j 0 100{   repeat i 100 0   {      set [i x] ([i (x+1)]-1)   }   pause 3   set x (x-1)}