// Raining lines -- (c)1999, Jared Schiffmanpaper 0number mod a b{   set d (a/b)   value (a-(b*d))}set H 0set Y 100forever{   // erase previous line   pen 0   line 0 (Y+1) 100 (Y+1)   pen (<mod (H+78) 10>*8)   // draw new line   line 0 Y 100 Y   set Y (Y-1)   same? Y (H+1)   {      set Y 100      set H (H+1)      repeat a 0  H      {         pen [0 (a+1)]         line 0 a 100 a      }      pen [0 0]      repeat a H 100      {         line 0 100 100 100      }   }   same? H 99   {      set H 0      set Y 100   }}