// Quantitative Donut -- Jared Schiffmanload dbngraphics.dbnnumber jsin x{   set z (x%564)   set d (x%282)   smaller? 157 d   {      set d (282-d)   }   set y (d-(d*d*d)/60000)   smaller? 282 z   {      set y (-1*y)   }   value y}set N 101repeat a 1 N{   set <array a> ((15*<jsin (a*6*3)>/100)+25)}set sum 0set max 0set min 99999repeat a 1 N{   set sum (sum+<array a>)   smaller? max <array a>   {      set max <array a>   }   smaller? <array a> min   {      set min <array a>   }}set avg (sum/100)paper 40repeat g min avg{   circle 50 50 g (50-((g-min)*50/(avg-min)))}repeat g avg max{   circle 50 50 g ((g-avg)*50/(max-avg))}set current 1set oldcurrent currentset erase [50 (50+<array current>)]pen 100forever{   circle 50 50 <array oldcurrent> erase   set erase [50 (50+<array current>)]   circle 50 50 <array current> 100   set oldcurrent current   set current (<mouse 1>+1)   smaller? current 1   {      set current 1   }   smaller? N current   {      set current N   }}