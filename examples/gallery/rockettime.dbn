// Rocket time -- Elise Copaper 100pen 80line 0 <time 2> 100 <time 2>pen 70line 0 <time 1> 20 <time 1>forever{   set dsec <time 4>   set sec <time 3>   set min <time 2>   set hour <time 1>   set mark dsec   notsame? (sec-1) min   {      notsame? (sec-1) hour      {         repeat a 0 mark         {            pen ((mark-a)*3)            line a sec a (sec-1)         }      }   }   same? (sec-1) min   {      repeat a 0 mark      {         set [a (sec-1)] (mark-a)      }   }   same? (sec-1) hour   {      repeat a 0 mark      {         set [a (sec-1)] (a*4)      }   }   repeat b mark 100   {      pen ((b-mark)*5)      line b sec b (sec+1)   }   same? sec 59   {      pen 100      line 0 60 80 60   }}