// Probing Cell -- Jared Schiffmanpaper 0set <array 1> 100set <array 2> 0set <array 3> 86set <array 4> 49set <array 5> 50set <array 6> 86set <array 7> 0set <array 8> 99set <array 9> -49set <array 10> 86set <array 11> -86set <array 12> 50set <array 13> -99set <array 14> 0set <array 15> -86set <array 16> -49set <array 17> -50set <array 18> -86set <array 19> 0set <array 20> -99set <array 21> 49set <array 22> -86set <array 23> 86set <array 24> -50set posX 50set posY 50set randArm 4set anim 1set mode 0set toPosX 50set toPosY 50set coast 0forever{   paper 100   repeat a 0 11   {      set x0 (posX+ (<array (a*2+1)>/10))      set y0 (posY+ (<array (a*2+2)>/10))      set x1 (posX+ (<array (((a+1)%12)*2+1)>/10))      set y1 (posY+ (<array (((a+1)%12)*2+2)>/10))      pen 10      line x0 y0 x1 y1      set mul 10      same? a randArm      {         set mul anim         same? mode 0         {            set anim (anim+1)         }      }      set nX (x0 + (<array (a*2+1)>*mul/200))      set nY (y0 + (<array (a*2+2)>*mul/200))      pen 40      line x0 y0 nX nY      same? randArm a      {         set prevMode mode         smaller? nX 0         {            set mode 1         }         notsmaller? nX 100         {            set mode 1         }         smaller? nY 0         {            set mode 1         }         notsmaller? nY 100         {            set mode 1         }         notsame? prevMode mode         {            set toPosX nX            set toPosY nY         }      }   }   //	field toPosX toPosY (toPosX+2) (toPosY+2) 70   same? mode 1   {      set posX ((posX*95+toPosX*5)/100)      set posY ((posY*95+toPosY*5)/100)      smaller? posX 15      {         set posX 15      }      smaller? posY 15      {         set posY 15      }      notsmaller? posX 85      {         set posX 75      }      notsmaller? posY 85      {         set posY 85      }      set coast (coast+1)      notsmaller? coast 50      {         set coast 0         set mode 0         set anim 1         set t <time 4>         set randArm (t%12)      }   }}