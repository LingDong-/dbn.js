// Amoebic cellophane -- Elise Coload dbngraphics.dbncommand setpoint n x y{   set i ((n*2)+1)   set <array i> x   set <array (i+1)> y}command connect p1 p2{   set n1 ((p1*2)+1)   set n2 ((p2*2)+1)   line <array n1> <array (n1+1)> <array n2> <array (n2+1)>}number getx p{   value <array ((p*2)+1)>}number gety p{   value <array ((p*2)+2)>}command translate p dx dy{   set x (<getx p>+dx)   notsmaller? x 100   {      set x 99   }   smaller? x 0   {      set x 0   }   set y (<gety p>+dy)   notsmaller? y 100   {      set y 99   }   smaller? y 0   {      set y 0   }   setpoint p x y}setpoint 0 37 37setpoint 1 32 48setpoint 2 37 61setpoint 3 48 64setpoint 4 60 62setpoint 5 62 59setpoint 6 61 50setpoint 7 54 40setpoint 8 51 36setpoint 9 42 37command amoeba{   repeat a 0 8   {      connect a (a+1)      connect 0 9   }}command superamoeba{   repeat a 0 9   {      //pen (50-(a*5))      repeat b 0 9      {         connect a b      }   }}number mymod a1 b1{   value (a1-((a1/b1)*b1))}set seed (<time 3>+<time 4>)set val 0command newRandom{   // make a new random top   set seed ( (seed+3) * 541 )   // shift seed if it went negative   smaller? seed 0   {      set seed (seed+2147483647)   }   set val ( seed - ((seed/101) * 101) )}number random range{   newRandom   set r (((val-50)*range)/50)   value r}command nucleus{   set nx 0   set ny 0   repeat a 0 9   {      set nx (nx+<getx a>)      set ny (ny+<gety a>)   }   set nx (nx/10)   set ny (ny/10)   circle nx ny 7 80   circle nx ny 5 80}pen 100set mark 0forever{   set dx <random 20>   set n (mark-1)   smaller? n 0   {      set n 9   }   set dy <random 20>   translate mark dx dy   translate n ((dx*2)/3) ((dy*2)/3)   set n (mark+1)   notsmaller? n 10   {      set n 0   }   translate n ((dx*2)/3) ((dy*2)/3)   repeat a 0 9   {      translate a (dx/3)(dy/3)   }   paper 0   pen 32   superamoeba   pen 100   nucleus   amoeba   set mark (mark+1)   notsmaller? mark 10   {      set mark 0   }}