}
command testdbnletters
{
  paper 0
  pen 100
  set x 17
  set y 73
  set dx 12
  set dy 12
  set n 0
  repeat a 1 25
  {
    letter a x y
    set x (x+dx)
    set n (n+1)
    same? n 5
    {
      set x 17
      set y (y-dy)
      set n 0
    }
  }
  set x (x+dx*5)
  letter 26 x y
}

